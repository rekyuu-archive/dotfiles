[frame]
    width = 2
    color = "#EEE6E3"

[urgency_low]
    # IMPORTANT: colors have to be defined in quotation marks.
    # Otherwise the "#" and following would be interpreted as a comment.
    background = "#40388E"
    foreground = "#EEE6E3"
    timeout = 10

[urgency_normal]
    background = "#0F151D"
    foreground = "#EEE6E3"
    timeout = 10

[urgency_critical]
    background = "#C43F44"
    foreground = "#EEE6E3"
    timeout = 0

[global]
    font = Hack 11
