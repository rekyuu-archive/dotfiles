[frame]
    width = 2
    color = "#e8e8e8"

[urgency_low]
    # IMPORTANT: colors have to be defined in quotation marks.
    # Otherwise the "#" and following would be interpreted as a comment.
    background = "#ba8baf"
    foreground = "#e8e8e8"
    timeout = 10

[urgency_normal]
    background = "#181818"
    foreground = "#e8e8e8"
    timeout = 10

[urgency_critical]
    background = "#ab4642"
    foreground = "#e8e8e8"
    timeout = 0

[global]
    font = Dina 10
